<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    	
		<class name="po.Emp" table="EMP" lazy="true" >
	
	
	
			<id name="empNo" type="java.lang.Integer">
	    		
	    			<column name="empno"></column>
	    			<generator class="increment"	></generator>
	    		
	    		
	    		
	    	</id>
	    	<property name="empName" type="java.lang.String">
	    		
	    		<column name="ename"></column>
	    		
	    		
	    	</property>
	    	<property name="job" type="java.lang.String">
	    		
	    		<column name="job"></column>
	    	</property>
	    	
	    	<property name="salary" type="java.lang.Double">
	    		
	    		<column name="sal"></column>
	    		
	    	</property>
	    		
	    	<property name="hireDate" type="java.util.Date">
	    		<column name="hiredate"></column>
	    		
	    	</property>
	    	
	    	<many-to-one name="dept" class="po.Dept" column="DEPTNO" >

	    		
	    		
	    	</many-to-one>
	
	
	
	
	
	
	
	
		</class>
    
    
    
    
    
    
    
    
    
    
    
    
    
    </hibernate-mapping>
    
    